{
    "commands": [
    {
        "command": "trivia",
        "description": "Starts a new trivia game",
        "function": "startTrivia"
    },
    {
        "command": "scores",
        "description": "Shows scores for running trivia game",
        "function": "showTriviaScores"
    },
    {
        "command": "triviaStop",
        "description": "Stops the currently running trivia game",
        "function": "stopTrivia"
    },
    {
        "command": "photoshop",
        "description": "Replies with a random photoshop",
        "function": "getPhotoshop"
    },
    {
        "command": "melbournememe",
        "description": "Replies with a random meme from melbourne",
        "function": "handleMelbourneMeme"
    },
    {
        "command": "money",
        "description": "Shows how much money you have",
        "function": "showMoney",
        "parameters": [
            {
                "type": "Integer",
                "ignoreSpaces": false
            }
        ]
    },
    {
        "command": "betRoll",
        "description": "Bet some of your money and roll a die. If the die is 80 or higher, win money!",
        "function": "betRoll"
    },
    {
        "command": "help",
        "description": "Shows the command list",
        "function": "showCommandList"
    }]
}